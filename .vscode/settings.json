{
    "eslint.workingDirectories": [
        {
            "directory": "app"
        }
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": true
    },
    "eslint.validate": [
        "typescript",
        "javascript",
        "typescriptreact"
    ],
    "cSpell.words": [
        "Hacky",
        "choicesjs",
        "configurator",
        "configurators",
        "mixins",
        "selectboxes"
    ],
    "typescript.tsdk": "app\\node_modules\\typescript\\lib",
    "jest.rootPath": "app",
    "jest.debugCodeLens.showWhenTestStateIn": [
        "fail",
        "unknown",
        "pass",
        "skip"
    ],
    "jest.jestCommandLine": "npm run test:unit --"
}